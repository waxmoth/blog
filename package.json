{
  "name": "waxmoth-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "hexo generate && hexo server",
    "build": "hexo generate",
    "clean": "hexo clean",
    "deploy": "hexo deploy",
    "server": "hexo server",
    "create-draft": "hexo new draft $1",
    "publish": "hexo publish $1 post",
    "watch": "npm-watch"
  },
  "hexo": {
    "version": "6.3.0"
  },
  "dependencies": {
    "hexo": "^6.3.0",
    "hexo-generator-archive": "^1.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-index": "^2.0.0",
    "hexo-generator-tag": "^1.0.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-marked": "^5.0.0",
    "hexo-renderer-stylus": "^2.1.0",
    "hexo-server": "^3.0.0",
    "hexo-theme-landscape": "^0.0.3"
  },
  "devDependencies": {
    "npm-watch": "^0.11.0"
  },
  "watch": {
    "dev": {
      "patterns": [
        "source"
      ],
      "extensions": "md",
      "quiet": false
    }
  }
}